{
  "base_url": {
    "description": "即梦2 API 基础地址（例如：http://localhost:5100）",
    "type": "string",
    "default": "http://localhost:5100",
    "obvious_hint": true
  },
  "session_tokens": {
    "description": "API 授权 Session 列表（Authorization: Bearer），支持轮询以扩容额度",
    "type": "list",
    "hint": "YOUR_SESSION_ID",
    "default": ["YOUR_SESSION_ID_1", "YOUR_SESSION_ID_2"],
    "obvious_hint": true
  },
  "session_token": {
    "description": "单个 Session（兼容旧版）",
    "type": "string",
    "default": "",
    "invisible": true
  },
  "model": {
    "description": "默认模型名称",
    "type": "string",
    "default": "jimeng-4.0",
    "options": ["jimeng-4.0"]
  },
  "default_ratio": {
    "description": "默认图像比例",
    "type": "string",
    "default": "1:1",
    "options": ["1:1", "4:3", "3:4", "16:9", "9:16", "3:2", "2:3", "21:9"]
  },
  "default_resolution": {
    "description": "默认分辨率（1k/2k/4k）",
    "type": "string",
    "default": "2k",
    "options": ["1k", "2k", "4k"]
  },
  "negative_prompt": {
    "description": "默认负面提示词（可选）",
    "type": "string",
    "default": ""
  },
  "sample_strength": {
    "description": "图生图采样强度（0.0-1.0）",
    "type": "float",
    "default": 0.7
  },
  "response_format": {
    "description": "响应格式（内部使用）",
    "type": "string",
    "default": "url",
    "options": ["url", "b64_json"],
    "invisible": true
  },
  "callback_api_base": {
    "description": "AstrBot 回调文件服务（内部使用）",
    "type": "string",
    "default": "",
    "invisible": true
  },
  "nap_server_address": {
    "description": "NAP cat 服务地址（仅跨机文件转发场景需要）",
    "type": "string",
    "default": "localhost"
  },
  "nap_server_port": {
    "description": "NAP cat 文件端口（仅跨机文件转发场景需要）",
    "type": "int",
    "default": 3658
  },
  "max_retry_attempts": {
    "description": "HTTP 请求最大重试次数（网络抖动时）",
    "type": "int",
    "default": 3
  },
  "video_model": {
    "description": "默认视频模型名称（如 jimeng-video-3.0）",
    "type": "string",
    "default": "jimeng-video-3.0",
    "options": [
      "jimeng-video-3.0-pro",
      "jimeng-video-3.0",
      "jimeng-video-2.0-pro",
      "jimeng-video-2.0"
    ]
  },
  "video_stream": {
    "description": "是否使用流式（SSE）模式生成视频链接",
    "type": "bool",
    "default": true
  }
}
