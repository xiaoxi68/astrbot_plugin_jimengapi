{
  "base_url": {
    "description": "即梦2 API 基础地址（例如：http://localhost:5100）",
    "type": "string",
    "default": "http://localhost:5100",
    "obvious_hint": true
  },
  "session_tokens": {
    "description": "API 授权 Session 列表（Authorization: Bearer），支持轮询以扩容额度",
    "type": "list",
    "hint": "YOUR_SESSION_ID",
    "default": ["YOUR_SESSION_ID_1", "YOUR_SESSION_ID_2"],
    "obvious_hint": true
  },
  "session_token": {
    "description": "单个 Session（兼容旧版）",
    "type": "string",
    "default": "",
    "invisible": true
  },
  "model": {
    "description": "默认模型名称",
    "type": "string",
    "default": "jimeng-4.0",
    "options": [
      "jimeng-4.0",
      "jimeng-3.1",
      "jimeng-3.0",
      "jimeng-2.1"
    ]
  },
  "default_ratio": {
    "description": "默认图像比例",
    "type": "string",
    "default": "1:1",
    "options": ["1:1", "4:3", "3:4", "16:9", "9:16", "3:2", "2:3", "21:9"]
  },
  "default_resolution": {
    "description": "默认分辨率（1k/2k/4k）",
    "type": "string",
    "default": "2k",
    "options": ["1k", "2k", "4k"]
  },
  "negative_prompt": {
    "description": "默认负面提示词（可选）",
    "type": "string",
    "default": ""
  },
  "sample_strength": {
    "description": "图生图采样强度（0.0-1.0）",
    "type": "float",
    "default": 0.7
  },
  "response_format": {
    "description": "响应格式（内部使用）",
    "type": "string",
    "default": "url",
    "options": ["url", "b64_json"],
    "invisible": true
  },
  "callback_api_base": {
    "description": "AstrBot 回调文件服务（内部使用）",
    "type": "string",
    "default": "",
    "invisible": true
  },
  "nap_server_address": {
    "description": "NAP cat 服务地址（仅跨机文件转发场景需要）",
    "type": "string",
    "default": "localhost"
  },
  "nap_server_port": {
    "description": "NAP cat 文件端口（仅跨机文件转发场景需要）",
    "type": "int",
    "default": 3658
  },
  "max_retry_attempts": {
    "description": "HTTP 请求最大重试次数（网络抖动时）",
    "type": "int",
    "default": 3
  },
  "video_model": {
    "description": "默认视频模型名称（如 jimeng-video-3.0）",
    "type": "string",
    "default": "jimeng-video-3.0",
    "options": [
      "jimeng-video-3.0-pro",
      "jimeng-video-3.0",
      "jimeng-video-2.0-pro",
      "jimeng-video-2.0"
    ]
  },
  "video_stream": {
    "description": "是否使用流式（SSE）模式生成视频链接",
    "type": "bool",
    "default": true
  },
  "group_list_mode": {
    "description": "群名单模式（whitelist=白名单，blacklist=黑名单）",
    "type": "string",
    "default": "blacklist",
    "options": ["whitelist", "blacklist"]
  },
  "groups": {
    "description": "群名单（依据模式为白/黑名单）",
    "type": "list",
    "default": []
  },
  "rate_limit_enabled": {
    "description": "是否启用每群频率限制",
    "type": "bool",
    "default": false
  },
  "rate_limit_window_minutes": {
    "description": "频率限制窗口（分钟）",
    "type": "int",
    "default": 10
  },
  "rate_limit_max_calls": {
    "description": "每个窗口内最大调用次数（每群）",
    "type": "int",
    "default": 5
  },
  "cleanup_enabled": {
    "description": "是否启用定时清理缓存(images/videos)",
    "type": "bool",
    "default": true
  },
  "cleanup_every_days": {
    "description": "清理执行周期（天）",
    "type": "int",
    "default": 3
  },
  "log_level": {
    "description": "日志级别",
    "type": "string",
    "default": "INFO",
    "options": ["ERROR", "WARN", "INFO", "DEBUG"]
  },
  "log_verbose": {
    "description": "输出更详细的运行日志",
    "type": "bool",
    "default": true
  },
  "log_trace_http": {
    "description": "记录HTTP请求/响应关键字段（勿记录敏感信息）",
    "type": "bool",
    "default": false
  },
  "log_media_headers": {
    "description": "记录媒体下载响应头(仅关键信息)",
    "type": "bool",
    "default": false
  },
  "log_policy": {
    "description": "记录群策略与限流判定",
    "type": "bool",
    "default": true
  },
  "log_timing": {
    "description": "记录关键耗时(生成/下载/发送)",
    "type": "bool",
    "default": true
  }
}

